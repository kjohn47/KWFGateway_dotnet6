{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "EnableGatewayLogs": true,
  "EnableHttpClientLogs": true,
  "AuthorizationConfig": {
    "Ports": [
      5047
    ],
    "BaseUrl": "https://localhost",
    "ValidateSessionEndpoint": "validate-session",
    "AuthorizationHeader": "",
    "LoginRequiredCode": "LoginRequired",
    "RedirectTokenFromRequest": true,
    "Timeout": 5000,
    "CertificateConfiguration": {
      "CertificateSource": "NotSet",
      "CertificateValue": "",
      "CertificatePassword": "",
      "SslProtocols": "None"
    }
  },
  "AllowedHosts": "*",
  "ApiConfigurationPath": null,
  "ApiConfigurationUseCurrentDomainPath": false,
  "FaviconConfiguration": {
    "UseFavicon": true,
    "FaviconPath": null,
    "FaviconUseCurrentDomainPath": false
  },
  "GlobalCertificateConfiguration": null
}
