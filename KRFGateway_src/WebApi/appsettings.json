{
  "AppConfiguration": {
    "ApiName": "KRFGateway",
    "TokenIdentifier": "AccessToken",
    "TokenKey": "APICOMMONSIGNEDKEY",
    "TokenValidateLife": true,
    "EnableReqLogs": false,
    "RequestBufferSize": null,
    "LocalizationConfiguration": {
      "LocalizationCode": "pt-PT",
      "CurrencyLocalizationCode": null
    },
    "CorsConfiguration": {
      "AllowAnyOrigin": true,
      "AllowAnyHeader": true,
      "AllowAnyMethod": true,
      "AllowedOrigins": [ "http://localhost:3000", "https://localhost:3000" ],
      "AllowedHeaders": [ "Authorization", "content-type" ],
      "AllowedMethods": [ "GET", "POST" ]
    }
  },
  "Logging": {
    "IncludeScopes": {},
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System": "Warning"
    }
  },
  "KRFLogger": {
    "KRFLogDestination": [ "ToConsole", "ToDebug", "ToEvents", "ToEventSource", "ToFile" ],
    "KRFFileLogger": {
      "Path": null,
      "FilterLogLevelArray": [ "Critical", "Error" ],
      "DisableApiLogs": true,
      "DisableSQLLogs": true
    }
  },
  "AllowedHosts": "*",
  "KestrelConfiguration": {
    "HttpPort": 4700,
    "HttpsPort": 14700
  },
  "ConfigurationPath": "Configurations",
  "SessionServerEnabled": false,
  "SessionServer": {
    "CheckSessionUrl": "https://localhost:14741/Session/CheckSessionAlive",
    "Timeout": 130,
    "CertificatePath": "",
    "CertificateKey": "",
    "ForceDisableSSL": true
  }
}
